<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="com.imageshop.mapper.LoginLogMapper">
 	
 	<insert id="create">
 		insert into login_log(user_no,user_id,remote_addr)
 		values(#{userNo},#{userId},#{remoteAddr})
 	
 	</insert>
 </mapper>